---
import AstroHeadbob from "./AstroHeadbob.astro";

const currentPage = Astro.url.pathname;
let textColor = "var(--primary-dark)";

switch (currentPage) {
  case "/":
    // home page
    textColor = "var(--color-darkpurple)";
    break;
  case "/about":
    // about page
    textColor = "var(--color-darkgreen)";
    break;
  case "/work-with-us":
    // work with us page
    textColor = "var(--color-navy)";
    break;
  case "/contact":
    // contact page
    textColor = "var(--color-darkgreen)";
    break;
  default:
    // handle cases where the current page is not any of the above
    textColor = "var(--primary-dark)";
}
